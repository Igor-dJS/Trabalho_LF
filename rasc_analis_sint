PROG -> EOF
PROG -> DVAR SCOL
PROG -> DCONS SCOL
PROG -> DFN SCOL

DVAR -> VAR ID COL TYPE SCOL
DVAR -> VAR ID COL TYPE ATRIB LITERALINT SCOL
DVAR -> VAR ID COL TYPE ATRIB LITERALFLOAT SCOL

DCONS -> VAR ID COL TYPE ATRIB LITERALINT SCOL
DCONS -> CONST ID COL TYPE ATRIB LITERALFLOAT SCOL

DFN -> FN ID OPAR PARMS CPAR COL TYPE OBRAC DCS CMDS RETURN SCOL CBRAC

PARMS -> ID COL TYPE SCOL PARMS
PARMS -> ID COL TYPE

TYPE -> BOLL
TYPE -> INT
TYPE -> FLOAT

DCS(...)
CMDS(...)


